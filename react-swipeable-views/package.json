{
  "name": "SwipeableViewsDocs",
  "privte": true,
  "scripts": {
    "lint": "eslint . && echo \"eslint: no lint errors\"",
    "test": "npm run lint && npm run test:unit && npm run flow",
    "test:unit": "NODE_ENV=release babel-node test/unit.js",
    "test:watch": "NODE_ENV=release babel-node test/watch.js",
    "docs:browser:development": "NODE_ENV=docs-browser-development babel-node ./node_modules/.bin/webpack-dev-server --config=docs/webpack/developmentConfig.js --progress",
    "docs:browser:production": "rm -rf docs/dist && NODE_ENV=docs-browser-production babel-node ./node_modules/.bin/webpack --config=docs/webpack/productionConfig.js --progress",
    "docs:native:development": "NODE_ENV=docs-native-development node node_modules/react-native/local-cli/cli.js start --reset-cache --verbose",
    "flow": "flow",
    "bootstrap": "lerna bootstrap && npm run build",
    "build": "rm -rf packages/*/lib && NODE_ENV=release lerna exec -- babel src --out-dir lib --ignore spec.js",
    "watch": "lerna exec --concurrency 99 -- babel src --out-dir lib --watch",
    "publish": "npm run build && lerna exec -- npm run prepublish && lerna publish"
  },
  "author": "Olivier Tassinari <olivier.tassinari@gmail.com> (https://github.com/oliviertassinari)",
  "license": "MIT",
  "dependencies": {
    "react": "^15.4.2",
    "react-native": "^0.41.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.12",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-native": "^1.9.1",
    "babel-preset-stage-1": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.37.0",
    "force-case-sensitivity-webpack-plugin": "^0.2.1",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.10.0",
    "lerna": "2.0.0-beta.32",
    "less": "^2.7.2",
    "less-loader": "^2.2.1",
    "material-ui": "^0.16.7",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-native-button": "^1.7.1",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.22.0",
    "sinon": "^1.17.7",
    "stats-webpack-plugin": "^0.4.3",
    "style-loader": "^0.13.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
